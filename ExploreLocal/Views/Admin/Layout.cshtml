    </div>
    <script src="~/AdminTemplate/dist/libs/jquery/dist/jquery.min.js"></script>
    <script src="~/AdminTemplate/dist/libs/simplebar/dist/simplebar.min.js"></script>
    <script src="~/AdminTemplate/dist/libs/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/AdminTemplate/dist/js/app.min.js"></script>
    <script src="~/AdminTemplate/dist/js/app.minisidebar.init.js"></script>
    <script src="~/AdminTemplate/dist/js/app-style-switcher.js"></script>
    <script src="~/AdminTemplate/dist/js/sidebarmenu.js"></script>
    <script src="~/AdminTemplate/dist/js/custom.js"></script>
    <script src="~/AdminTemplate/dist/libs/owl.carousel/dist/owl.carousel.min.js"></script>
    <script src="~/AdminTemplate/dist/libs/apexcharts/dist/apexcharts.min.js"></script>
    <script src="~/AdminTemplate/dist/js/dashboard.js"></script>
